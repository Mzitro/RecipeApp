<Window x:Class="FinalPOE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalPOE"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/tiramisu-5171478_1280.jpg"/>
        </Grid.Background>
        <Frame x:Name="MainContentFrame" NavigationUIVisibility="Hidden" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>

        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button x:Name="Continue" Height="40" Width="150" Content="Continue" Click="Button_Click" Tag="Page1.xaml" Margin="0,0,0,0"/>
        </StackPanel>
    </Grid>
</Window>